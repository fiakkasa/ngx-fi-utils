{
  "name": "ngx-fi-utils",
  "license": "MIT",
  "description": "Various utilities and wrappers for angular projects",
  "author": {
    "name": "Adamos Fiakkas",
    "email": "fiakkasa@live.com",
    "url": "https://fiakkas.net"
  },
  "version": "0.0.2",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build": "ng build",
    "test": "ng test",
    "lint": "ng lint",
    "e2e": "ng e2e",
    "generate-all": "npm run generate-ngx-fi-lodash && npm run generate-ngx-fi-moment && npm run generate-ngx-fi-validator",
    "generate-ngx-fi-lodash": "ng build ngx-fi-lodash && cd ./dist/ngx-fi-lodash && npm pack && move ngx-fi-lodash-*.tgz ../../packages/ngx-fi-lodash",
    "generate-ngx-fi-moment": "ng build ngx-fi-moment && cd ./dist/ngx-fi-moment && npm pack && move ngx-fi-moment-*.tgz ../../packages/ngx-fi-moment",
    "generate-ngx-fi-validator": "ng build ngx-fi-validator && cd ./dist/ngx-fi-validator && npm pack && move ngx-fi-validator-*.tgz ../../packages/ngx-fi-validator"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "~8.1.0",
    "@angular/common": "~8.1.0",
    "@angular/compiler": "~8.1.0",
    "@angular/core": "~8.1.0",
    "@angular/forms": "~8.1.0",
    "@angular/platform-browser": "~8.1.0",
    "@angular/platform-browser-dynamic": "~8.1.0",
    "@angular/router": "~8.1.0",
    "ajv": "^6.10.0",
    "lodash": "^4.17.11",
    "moment": "^2.24.0",
    "rxjs": "~6.5.2",
    "tslib": "^1.10.0",
    "zone.js": "~0.9.1"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~0.801.0",
    "@angular-devkit/build-ng-packagr": "~0.801.0",
    "@angular/cli": "~8.1.0",
    "@angular/compiler-cli": "~8.1.0",
    "@angular/language-service": "~8.1.0",
    "@types/jasmine": "~3.3.13",
    "@types/jasminewd2": "~2.0.6",
    "@types/lodash": "^4.14.135",
    "@types/node": "~10.14.10",
    "codelyzer": "^5.1.0",
    "jasmine-core": "~3.4.0",
    "jasmine-spec-reporter": "~4.2.1",
    "karma": "~4.1.0",
    "karma-chrome-launcher": "~2.2.0",
    "karma-coverage-istanbul-reporter": "~2.0.5",
    "karma-jasmine": "~2.0.1",
    "karma-jasmine-html-reporter": "^1.4.2",
    "ng-packagr": "^5.3.0",
    "protractor": "~5.4.2",
    "ts-node": "~8.3.0",
    "tsickle": "^0.35.0",
    "tslint": "~5.18.0",
    "typescript": "~3.4.5"
  }
}
